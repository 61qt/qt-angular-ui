{
  "name": "qt-angular-ui",
  "version": "3.0.0",
  "license": "MIT",
  "description": "angular@1.x.x components for qt61.cn",
  "homepage": "https://github.com/61qt/qt-angular-ui",
  "main": "./libs/index.js",
  "style": "./libs/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/61qt/qt-angular-ui.git"
  },
  "engines": {
    "node": ">= 6.2.2"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "yamllint": "yamllint .travis.yml",
    "eslint": "eslint --config .eslintrc.js --ext .js --fix './src/**/*.js' './scripts/*.js' './*.js'",
    "lint": "npm run yamllint && npm run eslint",
    "docs": "rimraf ./temporary/docs && babel-node ./scripts/docs.js",
    "docs-develop": "cross-env development=1 concurrently 'npm run docs' 'docsify serve ./docs'",
    "unitest": "karma start",
    "coveralls": "npm run test && cat ./coverage/lcov.info | coveralls",
    "test": "rimraf ./coverage ./.temporary && npm run unitest",
    "sample": "webpack --progress --profile --hide-modules --config ./webpack.sample.config.babel.js",
    "develop": "webpack-dev-server --hot --config ./webpack.develop.config.babel.js --content-base ./libs",
    "product": "webpack --progress --profile --hide-modules --config ./webpack.product.config.babel.js",
    "release": "npm run test && npm run product",
    "gh-page": "cpy './docs/*' ./gh-page/docs && cpy './sample/*' ./gh-page/sample",
    "start": "concurrently 'npm run develop' 'npm run docs-develop'"
  },
  "peerDependencies": {
    "docsify-cli": "^4.1.11"
  },
  "devDependencies": {
    "angular": "^1.6.3",
    "angular-mocks": "^1.6.3",
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "colors": "^1.1.2",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.3.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "docsify-cli": "^4.2.0",
    "documentation": "^5.3.5",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jquery": "^3.2.1",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.1.0",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "watch": "^1.0.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "yaml-lint": "^1.0.0"
  },
  "dependencies": {
    "angular": "^1.6.3",
    "blueimp-canvas-to-blob": "^3.7.0",
    "canvas-compress": "^1.1.5",
    "cropperjs": "^1.0.0-beta.2",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.5",
    "moment": "^2.20.1",
    "weixin-js-sdk": "^1.2.0"
  }
}
