{
  "name": "qt-angular-ui",
  "version": "1.0.0",
  "license": "MIT",
  "description": "青藤网 angular ui 组件库",
  "homepage": "http://www.61qt.cn",
  "repository": {
    "type": "git",
    "url": "git@github.com:61qt/qt-angular-ui.git"
  },
  "engines": {
    "node": ">= 6.2.2"
  },
  "scripts": {
    "eslint": "eslint ./src",
    "mocha": "mocha --require babel-polyfill --compilers js:babel-register ./unitest/*.spec.js",
    "browser-test": "babel-node ./unitest/browser.js",
    "test": "npm run eslint && npm run mocha",
    "doctor": "rimraf ./.temporary && npm run test  && npm run browser-test && rimraf ./.temporary",
    "coverage": "rimraf ./coverage && nyc --reporter=text --reporter=lcov --report-dir=./coverage/cli npm run test && npm run browser-test",
    "coveralls": "npm run coverage && babel-node ./src/coveralls.js",
    "build": "rimraf ./dist && babel ./src --out-dir ./dist",
    "start": "babel ./src --out-dir ./dist --watch"
  },
  "peerDependencies": {},
  "devDependencies": {
    "angular-mocks": "^1.6.3",
    "async": "^2.1.5",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "columnify": "^1.5.4",
    "eslint": "^3.17.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "fs-extra": "^2.0.0",
    "handlebars": "^4.0.6",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "ng-annotate-loader": "^0.2.0",
    "nyc": "^10.1.2",
    "restangular": "^1.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "angular": "1.x.x",
    "blueimp-canvas-to-blob": "^3.7.0",
    "bourbon": "^4.3.3",
    "cropperjs": "^1.0.0-beta.2"
  }
}
